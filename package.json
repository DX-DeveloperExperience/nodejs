{
  "name": "nodejs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "format": "prettier --write \"app/**/*.js\"",
    "lint": "eslint app/**/*.js",
    "retire": "retire -n -p",
    "start": "node ./app/index.js",
    "start:dev": "nodemon ./app/index.js",
    "test": "jest",
    "prettier-test": "prettier -l 'app/**/*.{js,css}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm audit --only=prod && run-s lint prettier-test test"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "dotenv": "6.1.0",
    "express": "4.16.4",
    "morgan": "1.9.1",
    "swagger-ui-express": "3.0.10",
    "winston": "3.1.0",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "eslint": "5.9.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "22.0.0",
    "husky": "1.2.0",
    "jest": "23.6.0",
    "nodemon": "1.18.6",
    "npm-run-all": "4.1.3",
    "prettier": "1.15.2",
    "supertest": "3.3.0"
  }
}
